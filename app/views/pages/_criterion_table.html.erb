<table class="table table-hover table-responsive table-bordered text-center my-5">
  <thead class="thead-dark">
    <tr>
      <th scope="col"><%= column %></th>
    </tr>
  </thead>
  <tbody>
    <% criterions.each do |criterion| %>
      <tr id="criterion-<%= criterion.id %>">
        <td><%= criterion.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>
